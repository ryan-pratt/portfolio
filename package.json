{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Simple portfolio/blog site for myself to mess around with vibe.d",
  "main": "index.js",
  "scripts": {
    "start": "npm run-script build && dub",
    "build": "npm run-script compile-styles && npm run-script compile-scripts",
    "compile-styles": "lessc src/styles/styles.less ./public/styles/site.css",
    "compile-scripts": "npx babel src/scripts --out-dir ./public/scripts --presets react-app/prod",
    "watch": "npx babel --watch src/scripts --out-dir ./public/scripts --presets react-app/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryan-pratt/portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryan-pratt/portfolio/issues"
  },
  "homepage": "https://github.com/ryan-pratt/portfolio#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2"
  }
}
